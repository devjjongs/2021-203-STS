package x.y.p36;

public class Foo {
	private Bar bar;
	private Baz baz;

	public Foo() {
		// defualt Constructor
	}
//	잠재적 오류가 존재하는 클래스

	public Foo(Bar bar, Baz baz) {
		System.out.println("Foo 인스턴스 생성 위치(주소) : " + Integer.toHexString(hashcode()));
		this.bar = bar;
		this.baz = baz;
		System.out.println("Foo(Bar bar, Baz baz");
	}
}